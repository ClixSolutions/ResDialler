<Files .htaccess>
order allow,deny
deny from all
</Files>

Options All -Indexes

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
#turn off HTTPS for uploadify.php
RewriteCond %{HTTPS} On
RewriteCond %{REQUEST_URI} ^(go_ce/js/uploadify/uploadify.php) 
RewriteRule (.*) http://%{HTTP_HOST}/go_ce/js/uploadify/uploadify.php [R=301,L]
</IfModule>
